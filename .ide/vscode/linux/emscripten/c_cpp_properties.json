{
    "configurations": [
        {
            "name": "Emscripten",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/third-party/emsdk/upstream/emscripten/system/include",
                "${workspaceFolder}/third-party/**/**"
            ],
            "defines": [
                "_DEBUG",
                "ICESDK_SDL2",
                "ICESDK_FMOD",
                "ICESDK_EMSCRIPTEN"
            ],
            "compilerPath": "${workspaceFolder}/third-party/emsdk/upstream/bin/clang.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}